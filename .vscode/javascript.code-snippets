			"\t\t// }]",
			"\t]),",
			"\tgetRelationLabelFunctions: new Map([",
			"\t\t// ['$4', (row) => $5]",
			"\t]),",
			"\torderBy: [$6]",
			"});",
			"",
			"export { actions, load };"
		]
