	"Sveltekit - DBTable Load Function": {
		"prefix": "@dbtable",
		"body": [
			"import { getServerFunctions } from '\\$lib/dbTable';",
			"",
			"const { actions, load } = await getServerFunctions('$1', {",
			"\tcolumns: new Map([",
			"\t\t// ['$2', {",
