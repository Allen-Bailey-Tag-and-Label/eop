	"Sveltekit - DBTable Load Function": {
		"prefix": "@sk-dbtable-load",
		"body": [
			"import { getLoadData } from '\\$lib/dbTable';",
			"",
			"export const load = async () => {",
			"	const dbTable = await getLoadData('$1', {",
			"		columnOverrides: new Map([$2])",
			"	});",
			"	return { dbTable };",
			"};"
		]
